{
    "name": "test/test",
    "type": "project",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/dotenv": "^6.0",
        "symfony/phpunit-bridge": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
